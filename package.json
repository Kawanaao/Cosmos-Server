  "scripts": {
    "setup:env": "cross-env FARM_IS_DEMO=$npm_config_demo",
    "start": "if if-env npm_config_docker=true; then npm run start:docker; else npm run start:native; fi",
    "start:native": "concurrently -kp '{{name}}>' -n 'FARM,SERV' -c 'bgYellow.bold,bgMagenta.bold' 'npm:start:client' 'npm:start:backend'",
    "start:client": "npm run setup:env farm",
    "start:backend": "COSMOS_CONFIG_FOLDER=./zz_test_config/ CONFIG_FILE=./config_dev.json EZ=UTC ACME_STAGING=true COSMOS_HTTP_PORT=8443 go run src/*.go",
    "prestart:docker": "docker rm -f cosmos-dev 2> /dev/null && if-env npm_config_clean=true && rm -rf /var/lib/cosmos || true",
    "start:docker": "npm run build:docker && docker run -it --rm --cap-add NET_ADMIN -p 7200:443 -p 8443:80 -p 443:443 -p 4242:4242/udp --privileged -v /var/run/docker.sock:/var/run/docker.sock -e COSMOS_LOG_LEVEL=DEBUG -v /:/mnt/host -h cosmos-dev -v /var/lib/cosmos:/config --name cosmos-dev cosmos-dev",
    "build": "if if-env npm_config_docker=true; then npm run build:docker; else npm run build:native; fi",
    "build:docker": "docker build --build-arg npm_config_demo=$npm_config_demo --tag cosmos-dev .",
    "build:native": "concurrently -kp '{{name}}>' -n 'FARM,SERV' -c 'bgYellow.bold,bgMagenta.bold' 'npm:build:client' 'npm:build:backend'",
    "build:client": "npm run setup:env farm build",
    "build:backend": "go build -o build/cosmos src/*.go"
  },
